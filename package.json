{
  "name": "oqoolai-monorepo",
  "version": "2.0.0",
  "description": "ðŸš€ OqoolAI Cloud IDE - Advanced AI-Powered Development Environment with Monaco Editor, Interactive Terminal, and Real-time Collaboration",
  "private": true,
  "workspaces": [
    "packages/cli",
    "packages/cloud-editor/frontend",
    "packages/cloud-editor/backend", 
    "packages/shared"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "dev:cli": "npm run dev --workspace=packages/cli",
    "dev:frontend": "npm run dev --workspace=packages/cloud-editor/frontend",
    "dev:backend": "npm run dev --workspace=packages/cloud-editor/backend",
    "dev:cloud": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:cli": "npm run build --workspace=packages/cli",
    "build:frontend": "npm run build --workspace=packages/cloud-editor/frontend",
    "build:backend": "npm run build --workspace=packages/cloud-editor/backend",
    "build:cloud": "npm run build:backend && npm run build:frontend",
    "start:cloud": "npm run start --workspace=packages/cloud-editor/backend",
    "install:all": "npm install && npm install --workspaces",
    "upgrade:all": "npm update && npm update --workspaces"
  },
  "keywords": [
    "oqoolai",
    "cloud-ide",
    "monaco-editor", 
    "react",
    "typescript",
    "ai-powered",
    "code-editor",
    "terminal",
    "file-explorer",
    "real-time",
    "collaboration",
    "vscode-like",
    "web-ide",
    "monorepo",
    "arabic",
    "cli-tools"
  ],
  "author": {
    "name": "Oqool AI Team",
    "email": "team@oqoolai.com",
    "url": "https://oqoolai.com"
  },
  "contributors": [
    "Amir Muhammad <amir@oqoolai.com>",
    "OqoolAI Development Team"
  ],
  "homepage": "https://oqoolai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/oqoolai/cloud-editor.git"
  },
  "bugs": {
    "url": "https://github.com/oqoolai/cloud-editor/issues",
    "email": "support@oqoolai.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "volta": {
    "node": "20.18.1",
    "npm": "10.8.2"
  },
  "packageManager": "npm@10.8.2"
}
